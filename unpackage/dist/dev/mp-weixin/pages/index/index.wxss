
.top {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	height: 100rpx;
	position: fixed;
	top: 0;
	background-color: white;
	z-index: 99;
}
.top view {
	padding: 5px;
	margin: 0 20rpx;
	width: 80rpx;
}
.menu_one image {
	width: 50rpx;
	height: 50rpx;
}
.menu_one {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
	margin: 20px 5px;
}
.menu_one>view {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-flow: column nowrap;
	        flex-flow: column nowrap;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 12px;
}

